[
'DocComment
if $Options.generateComments
	foreach $Comment in $Element.ownedComment
		$Lines = $Comment.body
		if $Lines <> ""
			$Prefix = "/** "
			foreach $Line in $Lines.AsCommentLineList
				write $Prefix & $Line.AsString
				$Prefix =  "\n" & $Indent & " * "
			next
			write  " */\n" & $Indent
		endif
	next
endif
]